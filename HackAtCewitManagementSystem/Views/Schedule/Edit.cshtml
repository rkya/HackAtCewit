@{
    ViewData["Title"] = "Add Event";
}
@model Schedule
<section class="schedule" id="schedule">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 my-auto">
                <div class="container-fluid">
                    <h3>Edit Event</h3>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 my-auto">
                <div class="container-fluid">
                    @using (Html.BeginForm())
                    {
                        <div>
                            @Html.LabelFor(m => m.EventTitle)
                            @Html.TextBoxFor(m => m.EventTitle)
                        </div>
                        <div>
                            @Html.LabelFor(m => m.EventDescription)
                            @Html.TextBoxFor(m => m.EventDescription)
                        </div>
                        <div>
                            @Html.LabelFor(m => m.StartTime)
                            @Html.TextBoxFor(m => m.StartTime)
                        </div>
                        <div>
                            @Html.LabelFor(m => m.EndTime)
                            @Html.TextBoxFor(m => m.EndTime)
                        </div>
                        <div>
                            @Html.LabelFor(m => m.Room)
                            @Html.TextBoxFor(m => m.Room)
                        </div>
                        <div>
                            @Html.LabelFor(m => m.Presenter)
                            @Html.TextBoxFor(m => m.Presenter)
                        </div>
                        <input type="submit" value="Edit" />
                    }
                </div>
            </div>
        </div>
    </div>
</section>
